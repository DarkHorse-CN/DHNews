<?xml version="1.0" encoding="utf-8"?>
<set xmlns:android="http://schemas.android.com/apk/res/android"
    android:ordering="sequentially">

    <objectAnimator
        android:duration="400"
        android:interpolator="@android:interpolator/decelerate_cubic"
        android:propertyName="pathData"
        android:valueFrom="
           M20,20 H100 V90 H20 V20"
        android:valueTo="
           M105,20 H185 V90 H105 V20"
        android:valueType="pathType" />

    <objectAnimator
        android:duration="400"
        android:interpolator="@android:interpolator/decelerate_cubic"
        android:propertyName="pathData"
        android:valueFrom="
           M105,20 H185 V90 H105 V20"
        android:valueTo="
           M105,115 H185 V195 H105 V115"
        android:valueType="pathType" />

    <objectAnimator
        android:duration="400"
        android:interpolator="@android:interpolator/decelerate_cubic"
        android:propertyName="pathData"
        android:valueFrom="
           M105,115 H185 V195 H105 V115"
        android:valueTo="
           M20,115 H100 V195 H20 V115"
        android:valueType="pathType" />

    <objectAnimator
        android:duration="400"
        android:interpolator="@android:interpolator/decelerate_cubic"
        android:propertyName="pathData"
        android:valueFrom="
           M20,115 H100 V195 H20 V115"
        android:valueTo="
           M20,20 H100 V90 H20 V20"
        android:valueType="pathType" />
</set>